{
  "name": "spexplorerjs",
  "version": "0.0.2",
  "description": "SharePoint Explorer (JavaScript)",
  "scripts": {
    "jquery": "cpx node_modules/jquery/dist/jquery.min.js public/refs/",
    "bootstrap": "cpx node_modules/bootstrap/dist/js/bootstrap.min.js public/refs/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run jquery && npm run bootstrap",
    "eslint": "eslint public\\components\\**\\*.js --fix",
    "comps": "node src\\scripts\\uglify.js",
