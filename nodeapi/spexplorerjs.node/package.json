{
	"name": "spexplorerjs.node",
	"version": "1.0.0",
	"description": "spexplorerjs.node",
	"main": "app.js",
	"typescript": "^3.0.1",
	"author": {
		"name": "Martin"
	},
	"scripts": {
		"api": "tsc -p ./tsconfig.Api.Common.json",
		"apiwatch": "tsc -w -p ./tsconfig.Api.Common.json",
		"pack": "tsc packer --esModuleInterop & node packer.js --run",
		"packwatch": "concurrently \"tsc -w packer --esModuleInterop\" \"nodemon -w packer.js packer.js\"",
		"amdbundle": "tsc -p ./tsconfig.Api.Amd.json --outFile ./dist/bundles/api.amdbundle.js",
		"build": "npm run api && npm run pack && npm run amdbundle",
		"compile": "tsc",
		"watchall": "concurrently \"npm run packwatch\" \"npm run apiwatch\""
	},
	"dependencies": {
		"concurrently": "^5.2.0",
		"debug": "^2.2.0",
		"sp-request": "^2.1.3",
		"typescript": "^3.1.6"
	},
	"devDependencies": {
		"@types/debug": "0.0.30",
		"@types/jquery": "^3.3.14",
		"@types/microsoft-ajax": "0.0.33",
		"@types/node": "^8.10.38",
		"@types/webpack": "^4.41.12",
		"eslint": "^6.8.0",
		"html-loader": "^0.5.5",
		"jquery": "^3.5.0",
		"jsdom": "^12.1.0",
		"sp-jsom-node": "^1.3.6",
		"ts-loader": "^4.5.0",
		"typescript-declaration-webpack-plugin": "^0.1.1",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.1.2"
	}
}